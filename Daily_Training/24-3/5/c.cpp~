#include <iostream>
#include <ctime>
#include <cstdlib>
 
int getRand(int min, int max);
 
int main() {
	 
	    srand(time(0));
	    freopen("out.txt", "w", stdin);
	    std::vector<int> a(10);
	    std::vector<int> b(10);
	    for (int i = 0; i < 10; i++) {
	    	a[i] = getRand(1, 10);
	    }
	    for (int i = 0; i < 10; i++) {
		    int x = getRand(1, 10);
	    	while (x == a[i]) x = getRand(1, 10);
		b[i] = x;
	    }
	    std::vector<int> c(100), d(100);
	    for (int i =0; i < n; i++) {
	    	c[i] = getRand(1, 10);
		d[i] = getRand(1, 10);
	    }
	  	printf("%d\n", 10);
		for (int i = 0; i < 10; i++) {
			printf("%d %d\n", a[i], b[i]);	
		}
		printf("%d\n", 100);
	for (int i = 0; i < 100; i++) {
		printf("%d %d\n", c[i], d[i]);
	}
	    fclose(stdin);
	    return 0;
}
 
// 左闭右闭区间
int getRand(int min, int max) {
	return ( rand() % (max - min + 1) ) + min ;
}
